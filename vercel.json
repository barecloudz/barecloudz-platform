{
  "functions": {
    "supabase/functions/invite-client/index.ts": {
      "runtime": "@vercel/deno"
    }
  },
  "rewrites": [
    {
      "source": "/api/functions/v1/(.*)",
      "destination": "/supabase/functions/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}